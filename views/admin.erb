<h2>Admin panel</h2>

<table class="table table-striped"> 
		<thead> 
			<tr> 
				<th>ID</th> 
				<th>Name</th> 
				<th>Phone</th> 
				<th>Address</th> 
				<th>Order</th> 
			</tr> 
		</thead> 

	<tbody>

		<% @all_orders.each do |x| %>
			<tr>
				<td><%= x.id %></td> 
				<td><%= x.name %></td> 
				<td><%= x.phone %></td> 
				<td><%= x.address %></td> 
				<td><%= x.orders_input %></td> 
			</tr>
		<% end %>

	</tbody> 
</table> 